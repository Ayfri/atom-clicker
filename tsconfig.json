{
	"compilerOptions": {
		"declaration": true,
		"lib": ["ESNext", "dom"],
		"module": "commonjs",
		"noImplicitAny": true,
		"outDir": "cache",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"rootDir": "src",
		"sourceMap": true,
		"target": "ESNext"
	},
	"exclude": ["node_modules"]
}
